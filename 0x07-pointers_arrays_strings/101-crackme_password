/**
 * find_password - Find the password for crackme2 executable.
 *
 * Return: The password string.
 */
const char *find_password(void)
{
    const char *password = "example_password";

    return password;
}

int main(void)
{
    const char *password = find_password();

    printf("%s", password);

    return 0;
}

